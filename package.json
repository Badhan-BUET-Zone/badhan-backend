    "debug": "~2.6.9",
    "deep-email-validator": "^0.1.18",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-async-router": "^0.1.15",
    "express-rate-limit": "^5.2.6",
    "express-validator": "^6.12.1",
